{
  "manifest_version": 2,
  "name": "Github Baal Syntax Highlight",
  "version": "1.0",
  "description": "baal syntax highlight",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions" : [ "tabs" ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["baal.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
  ]
}
